<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="307.0" prefWidth="762.0"
      style="-fx-background-color: darkslategray;" xmlns="http://javafx.com/javafx/8">
    <cursor>
        <Cursor fx:constant="WAIT"/>
    </cursor>
    <effect>
        <Blend/>
    </effect>
    <ImageView id="splashImage" fitHeight="213.0" fitWidth="763.0" pickOnBounds="true" preserveRatio="true"/>
    <VBox alignment="TOP_CENTER" spacing="10.0">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <ProgressBar id="loadInicator" prefHeight="20.0" prefWidth="446.0" style="-fx-progress-color: white;"/>
        <Label id="infoMsg" prefHeight="15.0" prefWidth="233.0" text="" textFill="WHITE"/>
    </VBox>
</VBox>
